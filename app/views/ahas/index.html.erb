<ul>
  <% @ahas.each do |aha| %>
  <li id="<%= dom_id(aha) %>">
    <%= aha.note %>
    <%= link_to image_tag('trash-can-icon.jpg', :size => '20x20'), aha_url(aha.id), :method => 'delete', :remote => true %>
    <%= link_to image_tag('pencil.png', :size => '20x20'), edit_aha_url(aha.id) %>
  </li>
  <% end %>
  <li>
    <%= form_for @aha, :remote => true do |f| %>
      <%= f.text_field :note %>
    <% end %>
  </li>
</ul>